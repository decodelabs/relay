{
    "name": "decodelabs/relay",
    "description": "Mail tools",
    "type": "library",
    "keywords": [ ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4",
        "decodelabs/exceptional": "^0.6.1"
    },
    "require-dev": {
        "decodelabs/phpstan-decodelabs": "^0.7.0",
        "decodelabs/tagged": "^0.20.1"
    },
    "conflict": {
        "decodelabs/tagged": "<0.20"
    },
    "suggest": {
        "decodelabs/tagged": "Required for the mail generator"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Relay\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.1.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
